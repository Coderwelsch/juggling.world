{
	"kind": "collectionType",
	"collectionName": "user_play_locations",
	"info": {
		"singularName": "user-play-location",
		"pluralName": "user-play-locations",
		"displayName": "User Play Location",
		"description": ""
	},
	"options": {
		"draftAndPublish": true
	},
	"pluginOptions": {},
	"attributes": {
		"location": {
			"type": "component",
			"repeatable": false,
			"component": "base.location"
		},
		"users": {
			"type": "relation",
			"relation": "manyToMany",
			"target": "plugin::users-permissions.user",
			"inversedBy": "userPlayLocations"
		},
		"name": {
			"type": "string",
			"required": true
		},
		"image": {
			"type": "media",
			"multiple": false,
			"required": false,
			"allowedTypes": [
				"images"
			]
		},
		"description": {
			"type": "richtext",
			"required": true
		},
		"userEvents": {
			"type": "relation",
			"relation": "oneToMany",
			"target": "api::user-event.user-event",
			"mappedBy": "where"
		},
		"groupEventsLocations": {
			"type": "relation",
			"relation": "oneToMany",
			"target": "api::user-group-event.user-group-event",
			"mappedBy": "location"
		}
	}
}
