{
  "kind": "collectionType",
  "collectionName": "user_group_events",
  "info": {
    "singularName": "user-group-event",
    "pluralName": "user-group-events",
    "displayName": "User Group Event",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "richtext",
      "required": true
    },
    "group": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::user-group.user-group",
      "inversedBy": "userGroupEvents"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "public": {
      "type": "boolean",
      "default": true
    },
    "admins": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "plugin::users-permissions.user",
      "inversedBy": "groupEventsAdmins"
    },
    "start": {
      "type": "datetime"
    },
    "end": {
      "type": "datetime"
    },
    "location": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::location.location",
      "inversedBy": "groupEventsLocations"
    }
  }
}
